{
  "kind": "collectionType",
  "collectionName": "s_headers",
  "info": {
    "singularName": "s-header",
    "pluralName": "s-headers",
    "displayName": "section Header"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "position": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "max": 1,
      "min": 1,
      "default": 1
    },
    "component": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "minLength": 6,
      "default": "Header",
      "maxLength": 6
    },
    "logoImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "logoTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Game Factory",
      "required": true
    },
    "logoUrl": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "/",
      "required": true
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.link",
      "required": true
    },
    "dropdownLinks": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.dropdown-link",
      "required": true
    }
  }
}
